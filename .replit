modules = ["python-3.11", "postgresql-16"]

[nix]
channel = "stable-25_05"
packages = ["cairo", "ffmpeg-full", "freetype", "ghostscript", "glibcLocales", "gobject-introspection", "gtk3", "libGL", "libGLU", "libsndfile", "libxcrypt", "pkg-config", "portaudio", "qhull", "tcl", "tk", "xsimd"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Eros Discord Bot"

[[workflows.workflow]]
name = "Eros Discord Bot"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python updated_cns_discord_bot.py"

[workflows.workflow.metadata]
outputType = "console"

[deployment]
deploymentTarget = "vm"
run = ["python", "updated_cns_discord_bot.py"]
ignorePorts = true

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8765
externalPort = 80

[agent]
expertMode = true
integrations = ["python_database:1.0.0", "google-mail:1.0.0"]
